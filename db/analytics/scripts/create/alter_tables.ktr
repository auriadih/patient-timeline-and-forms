    <connection>PostgreSQL parametrized 1</connection>
    <sql>with fields as (
		
select
	ff.form_field_id,
	ff.field_number_nesting_order,
	func.simplify_characters(a.field_analytics_name) as field_name,
	ff.field_type_name,
	case when ff.data_type_name = 'double' then 'double precision'
		when ff.data_type_name = 'char' then 'varchar'
		when ff.data_type_name = 'timestamp' then 'varchar'
		else ff.data_type_name end as data_type_name
	
	from wtf.v_form_fields as ff
	inner join wtf.v_analytics_field_synonyms as a
		on ff.form_field_id = a.form_field_id
	where ff.field_type_name != 'ei_vastausta'
	and ff.form_id = '${form_id}'::bigint
	order by ff.field_number_nesting_order),
field_choices as (
	select fields.form_field_id,
	fields.field_name,
	fields.field_type_name,
	fields.data_type_name,
	func.simplify_characters(concat_ws('_', fields.field_name, func.simplify_characters(ffc.choice_name))) as choice_name,
	choice_number
	from fields
	left outer join wtf.v_form_field_choices as ffc
		on fields.form_field_id = ffc.form_field_id
		and fields.field_type_name = 'monivalinta_monta_vastausta'
	order by fields.field_number_nesting_order, choice_number
)

select field_choices.form_field_id,
	case
		when field_choices.field_type_name != 'monivalinta_monta_vastausta'
		then concat('alter table ${schema_name}.${table_name} add column ', field_choices.field_name, ' ', field_choices.data_type_name, ';')
		else concat(string_agg(concat('alter table ${schema_name}.${table_name} add column ', field_choices.choice_name, ' boolean'),';'
			order by field_choices.choice_number), ';')
	end as alter_table_statement
from field_choices
group by field_choices.form_field_id,
	field_choices.field_type_name,
	field_choices.field_name,
	field_choices.data_type_name;

</sql>
