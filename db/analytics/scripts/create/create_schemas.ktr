    <connection>PostgreSQL parametrized 1</connection>
    <sql>
-- Prepare SQL commands for the following actions:

-- Create a schema for each user group.
-- Drop schemas if they exist.


with user_groups as
(select distinct group_id, group_name as group_name, func.simplify_characters(group_name) as schema_name
		from base.user_group as ug
		inner join wtf.form as f on ug.group_id = f.owner_group_id
		where group_name not in ('wtf','func','public', 'cdc', 'information_schema', 'pg_catalog')
)

select group_id, group_name, schema_name,
'drop schema if exists ' || user_groups.schema_name || ' cascade; create schema ' || user_groups.schema_name || ';'
as drop_and_create_schemas
from user_groups;
</sql>
